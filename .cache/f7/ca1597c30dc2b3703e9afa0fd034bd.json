ad = () => resolve(image);\n      image.onerror = (err) => reject(new Error(`Could not load image ${url}: ${err}`));\n    } catch (error) {\n      reject(error);\n    }\n  });\n}\n"},"lineCount":null}},"error":null,"hash":"d80eaf403a7f4b69aba35c845a65132f","cacheData":{"env":{}}}